<Window x:Class="PracticaUsuarios.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaUsuarios"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="800">
    <StackPanel>
		<TextBlock Margin="10" FontSize="22" FontFamily="Proxy 9"> Nuevo Usuario</TextBlock>
		<ComboBox SelectionChanged="ComboBreiker_SelectionChanged" Name="ComboBreiker" Margin="10" FontFamily="Proxy 9" FontSize="22">
			<ComboBoxItem>Alumno</ComboBoxItem>
			<ComboBoxItem>Maestro</ComboBoxItem>
		</ComboBox>
		
		<StackPanel Name="panelConfiguracion">
			
		</StackPanel>
		<Button  Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Center" FontSize="22" FontFamily="Proxy 9" Click="Button_Click"> Guardar</Button>
	</StackPanel>
</Window>
